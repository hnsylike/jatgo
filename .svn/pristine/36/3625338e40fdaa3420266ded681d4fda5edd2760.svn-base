#程序初始化时自动migrate控制，false表示项目启动不会执行默认的migrate动作
flyway.init.autoMigrate=false
spring.profiles.active=${ATGO_ENV} 
spring.flyway.baseline-on-migrate=true
spring.flyway.table=SCHEMA_VERSION
spring.flyway.check-location=true

spring.datasource.driver-class-name=com.mysql.jdbc.Driver
spring.datasource.url=jdbc:mysql://192.168.1.251:3306/ATGO_PROD?useSSL=false&characterEncoding=utf-8
spring.datasource.username=devwrite
spring.datasource.password=atgoPass

logging.path=log
